{
  "security": {
    "enabled": true,
    "encryption": {
      "algorithm": "AES-256-GCM",
      "key_rotation_days": 90
    },
    "input_validation": {
      "max_input_length": 10000,
      "sanitize_html": true,
      "validate_file_uploads": true
    },
    "authentication": {
      "session_timeout": 3600,
      "max_login_attempts": 5,
      "lockout_duration": 1800
    },
    "rate_limiting": {
      "requests_per_minute": 60,
      "burst_limit": 10
    },
    "cors": {
      "allowed_origins": [
        "https://stillme.ai"
      ],
      "allowed_methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ],
      "allowed_headers": [
        "Content-Type",
        "Authorization"
      ]
    }
  }
}