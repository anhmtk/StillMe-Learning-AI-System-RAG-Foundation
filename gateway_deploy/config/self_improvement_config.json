{
  "self_improvement": {
    "version": "1.0.0",
    "name": "Self Improvement Manager",
    "description": "Module tự cải thiện với cơ chế an toàn tối đa",
    "safety_level": "MAXIMUM"
  },
  "safety_rules": {
    "read_only_default": true,
    "require_approval": true,
    "four_eye_check": true,
    "sandbox_testing": true,
    "auto_rollback": true
  },
  "approval_process": {
    "ethical_core_required": true,
    "content_integrity_required": true,
    "test_suite_required": true,
    "manual_review_required": true
  },
  "sandbox": {
    "enabled": true,
    "timeout": 300,
    "memory_limit": "512MB",
    "isolated_environment": true
  },
  "backup": {
    "enabled": true,
    "auto_backup": true,
    "backup_retention": 7,
    "compression": true
  },
  "monitoring": {
    "track_changes": true,
    "performance_metrics": true,
    "safety_reports": true,
    "audit_log": true
  },
  "logging": {
    "level": "INFO",
    "format": "json",
    "file": "logs/self_improvement.log",
    "max_size": "10MB",
    "backup_count": 5
  },
  "limits": {
    "max_proposed_changes": 100,
    "max_file_size": "10MB",
    "max_operations_per_hour": 50,
    "cooldown_period": 300
  }
}